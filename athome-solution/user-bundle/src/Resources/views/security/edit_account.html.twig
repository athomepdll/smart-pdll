{% extends '@AthomeUser/layout.html.twig' %}

{% block title %}{{ 'change_password'|trans }} - {{ parent() }}{% endblock %}

{% block body %}
    {{ form_start(form) }}
    {{ form_row(form.email) }}
    {{ form_row(form.oldPassword) }}
    {{ form_row(form.newPassword) }}
    <button type="submit" value="submit">Submit</button>
    {{ form_end(form) }}
{% endblock %}
